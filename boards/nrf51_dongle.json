{
  "build": {
    "arduino":{
      "ldscript": "nrf51_xxac.ld"
    },
    "core": "nRF5",
    "cpu": "cortex-m0",
    "extra_flags": "-DNRF51_DONGLE -DDEVICE_RAM_SIZE=32 -DUSE_LFXO",
    "f_cpu": "16000000L",
    "mcu": "nrf51822_xxac",
    "variant": "nRF51_Dongle"
  },
  "connectivity": [
    "bluetooth"
  ],
  "debug": {
    "default_tools": [
      "jlink"
    ],
    "jlink_device": "nRF51822_xxAC",
    "onboard_tools": [
      "cmsis-dap",
      "jlink"
    ],
    "svd_path": "nrf51.svd"
  },
  "frameworks": [
    "arduino"
  ],
  "name": "Nordic nRF51 Dongle (PCA10031)",
  "upload": {
    "maximum_ram_size": 32768,
    "maximum_size": 258048,
    "protocol": "jlink",
    "protocols": [
      "cmsis-dap",
      "jlink",
      "nrfjprog"
    ]
  },
  "url": "https://www.nordicsemi.com/Products/Development-hardware/nRF51-Dongle",
  "vendor": "Nordic"
}
